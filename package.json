{
  "name": "paraview-lite",
  "version": "0.0.0-semantically-release",
  "description": "Lite ParaView client for Scientific Visualization on the Web",
  "author": "Kitware",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
